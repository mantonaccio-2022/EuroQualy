	'Codigo'																, ; //XB_DESCSPA
	'Code'																	, ; //XB_DESCENG
	''																		, ; //XB_WCONTEM
	'CT5_LANPAD'															} ) //XB_CONTEM

aAdd( aSXB, { ;
	'CT5'																	, ; //XB_ALIAS
	'4'																		, ; //XB_TIPO
